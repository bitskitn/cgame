/*   Piano44K Instrument   */

keymap Piano44K00key
{
    velocityMin = 0;
    velocityMax = 127;
    keyMin      = 0;
    keyMax      = 41;
    keyBase     = 36;  /* 51; */
    detune      = 0;
    sound       = Piano44K00;
}

